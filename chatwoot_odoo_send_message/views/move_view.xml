<odoo>
    <data>
   
        <record id="view_move_form_custom_button" model="ir.ui.view">
            <field name="name">move.view.form.inherit.chatwoot.redirect</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@name='button_box']" position="inside">
                    <div class="oe_button_box" name="button_box">
                        <button name="action_send_msg" string="Enviar WhatsApp"
                                type="object"
                                class="oe_stat_button" icon="fa-whatsapp"/>
                    </div>
                </xpath>
            </field>
        </record>

    </data>
</odoo>